{
  "FormFields": [
    {
      "label": "Name",
      "key": "name",
      "type": "text",
      "maxlength": 20,
      "placeholder": "Your Full Name",
      "required": true
    },
    {
      "label": "E-Mail",
      "key": "email",
      "type": "text",
      "placeholder": "Your Email",
      "required": false
    },
    {
      "label": "Country",
      "key": "nationality",
      "type": "select",
      "options": ["Egypt", "USA", "France"],
      "placeholder": "Select"
    },
    {
      "label": "Active",
      "key": "check",
      "type": "checkbox",
      "options": ["isActive"]
    }
  ]
}
